{
  "name": "reis_new",
  "private": true,
  "version": "4.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "concurrently --kill-others-on-fail \"npm:test:run\" \"npm run build:quick && npm run test:smoke\"",
    "build:all": "rm -rf dist && concurrently \"npm:build:app\" \"npm:build:content\"",
    "build:quick": "npm run build:all",
    "build:app": "vite build",
    "build:content": "BUILD_TARGET=content vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "xvfb-run playwright test",
    "test:e2e:headed": "xvfb-run playwright test --headed",
    "test:serenity": "xvfb-run playwright test e2e/serenity/specs",
    "test:smoke": "xvfb-run playwright test e2e/serenity/specs/smoke.spec.ts",
    "test:e2e:setup": "playwright install chromium",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "daisyui": "^5.5.8",
    "embla-carousel-react": "^8.6.0",
    "file-saver": "^2.0.5",
    "input-otp": "^1.4.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.554.0",
    "motion": "^12.23.24",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.4.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@serenity-js/assertions": "^3.37.1",
    "@serenity-js/console-reporter": "^3.37.1",
    "@serenity-js/core": "^3.37.1",
    "@serenity-js/playwright": "^3.37.1",
    "@serenity-js/playwright-test": "^3.37.1",
    "@serenity-js/web": "^3.37.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.1.31",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
